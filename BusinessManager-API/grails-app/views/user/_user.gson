import security.User
import java.util.Date


model {
	User user
}

json {
	id user.id
	phone user.phone
	name user.name
	email user.email
	username user.username
	birthDate user.birthDate.format("dd/MM/yyyy")
	contractDate user.contractDate.format("dd/MM/yyyy")
	cpf user.cpf
	hasTemporaryPassword user.hasTemporaryPassword
	enabled user.enabled
	imageBytes user.imageBytes
	contentType user.contentType
	company g.render(template: 'company', model: [company: user.company])
}
