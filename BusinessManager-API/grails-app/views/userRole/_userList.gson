import security.UserRole

model {
    UserRole userRole
}

json {
    key userRole.user.id
    name userRole.user.name
    email userRole.user.email
    username userRole.user.username
    cpf userRole.user.cpf
    phone userRole.user.phone
    birthDate userRole.user.birthDate.format("dd/MM/yyyy")
    contractDate userRole.user.contractDate.format("dd/MM/yyyy")
    enabled userRole.user.enabled
    permission userRole.role.authority
}