import security.User

model {
	User user
}

json {
	id user.id
	phone user.phone
	name user.name
	email user.email
	username user.username
	birthDate user.birthDate
	contractDate user.contractDate
	cpf user.cpf
	hasTemporaryPassword user.hasTemporaryPassword
	enabled user.enabled
	imageBytes user.imageBytes
	cotentType user.contentType
	company g.render(template: 'company', model: [company: user.company])
}
